<h1>Home#index</h1>
<p>Find me in app/views/home/index.html.erb</p>

<%= form_for @message, remote: true do |m| %>
    <%= m.text_field :content %>
    <%= m.submit %>
<% end %> 

<div id="messages">
<% for message in @messages %>
    <div class='message'><%= message.content %></div>
<% end %>
</div>

